<+><!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <title>Profile</title>\n    <link href=\"/static/css/bootstrap.css\" rel=\"stylesheet\">\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/css/intlTelInput.css\">\n    <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/intlTelInput.min.js\"></script>\n</head>\n<body>\n<form th:action=\"@{/userProfile}\" method=\"post\" th:object=\"${update}\" enctype=\"multipart/form-data\">\n    <input type=\"hidden\" th:field=\"*{id}\">\n<div class=\"container d-flex justify-content-center align-items-center\" style=\"height: 100vh;\">\n    <div class=\"row gutters\">\n        <div class=\"col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12\">\n            <div class=\"card h-100\">\n                <div class=\"card-body\">\n                    <div class=\"account-settings\">\n                        <div class=\"user-profile\">\n                            <div class=\"user-avatar d-flex justify-content-center align-items-center\">\n                                <img style=\"height: 250px; width: 250px; object-fit: cover; border-radius: 50%;\" th:src=\"${imageBase64 != null} ? 'data:image/png;base64,'+${imageBase64} : 'https://img.icons8.com/nolan/250/user-default.png'\" alt=\"user-default\"/>\n                            </div>\n                           <div class=\"d-flex justify-content-center align-items-center\">\n\n                            </div>\n                            <br>\n                            <div class=\"d-flex justify-content-center align-items-center\">\n                                <input type=\"file\" id=\"image\" name=\"image\" accept=\"image/png, image/jpeg\" style=\"display: none;\"/>\n                                <button type=\"button\" class=\"btn btn-primary\" onclick=\"document.getElementById('image').click();\">Change Photo</button>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"col-xl-9 col-lg-9 col-md-12 col-sm-12 col-12\">\n            <div class=\"card h-100\">\n                <div class=\"card-body\">\n                    <div class=\"row gutters\">\n                        <div th:if=\"${success}\" class=\"alert alert-success\" role=\"alert\">\n                            <p th:text=\"${success}\"></p>\n                        </div>\n                        <div th:if=\"${error}\" class=\"alert alert-danger\" role=\"alert\">\n                            <p th:text=\"${error}\"></p>\n                        </div>\n                        <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12\">\n                            <h6 class=\"mb-2 text-primary\">Personal Details</h6>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                            <label for=\"Username\" class=\"me-2\">Username :</label>\n                            <input type=\"text\" class=\"form-control\" th:value=\"${update.username}\" id=\"Username\" placeholder=\"Enter the username\">\n                            </div>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                                <label for=\"prenom\">Prenom</label>\n                                <input type=\"text\" class=\"form-control\" th:value=\"${update.prenom}\" th:field=\"${update.prenom}\" id=\"prenom\" placeholder=\"Entrer le prenom\">\n                            </div>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                                <label for=\"nom\">Nom</label>\n                                <input type=\"text\" class=\"form-control\" th:value=\"${update.nom}\" th:field=\"${update.nom}\" id=\"nom\" placeholder=\"Entrer le nom\">\n                            </div>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                                <label for=\"Email\">Email</label>\n                                <input type=\"email\" class=\"form-control\" th:value=\"${update.email}\" th:field=\"${update.email}\" id=\"Email\" placeholder=\"Entrer le Email\">\n                            </div>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                                <label for=\"Telephone\">Telephone</label>\n                                <div class=\"input-group\">\n                                    <input type=\"tel\" class=\"form-control w-100\" th:value=\"${update.telephone}\" th:field=\"${update.telephone}\" id=\"Telephone\" placeholder=\"Entrer le Nombre Telephone\">\n                                </div>\n                            </div>\n                        </div>\n                        <br>\n                    <div class=\"row gutters\">\n                    <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12\">\n                        <h6 class=\"mb-2 text-primary\">Password Management</h6>\n                    </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                        <button type=\"button\" class=\"btn btn-primary link-light\" id=\"resetPasswordButton\" onclick=\"location.href='/resetPassword'\">Reset Password</button>\n                        </div>\n                    </div>\n                    <div class=\"row gutters\">\n                        <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12\">\n                            <h6 class=\"mt-3 mb-2 text-primary\">Address</h6>\n                        </div>\n                        <div class=\"col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12\">\n                            <div class=\"form-group\">\n                                <label for=\"Ville\">Ville</label>\n                                <input type=\"text\" class=\"form-control\" th:value=\"${update.ville}\" th:field=\"${update.ville}\" id=\"Ville\" placeholder=\"Entrer la ville\">\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"row gutters\">\n                        <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12\">\n                            <div class=\"text-right\">\n                                <button type=\"submit\" name=\"submit\" class=\"btn btn-secondary\">Cancel</button>\n                                <button type=\"submit\" name=\"submit\" class=\"btn btn-primary\">Update</button>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</form>\n<script>\n    var input = document.querySelector(\"#Telephone\");\n    var iti = window.intlTelInput(input, {\n        allowDropdown: true,\n        initialCountry: \"auto\",\n        separateDialCode: true,\n        responseFields: \"iso2\",\n        formatOnDisplay: true,\n\n\n        geoIpLookup: function(success, failure) {\n            $.get(\"https://ipinfo.io\", function() {}, \"jsonp\").always(function(resp) {\n                var countryCode = (resp && resp.country) ? resp.country : \"\";\n                success(countryCode);\n            });\n        },\n    });\n\n    // Add the 'large-input' class to the input field\n    input.classList.add(\"form-control\");\n</script>\n</body>\n</html>
